<h1>New Post!</h1>

<form action="/posts" method="POST">
  <div><input type="text" placeholder="Title" name="post[title]"></div>
  <div><input type="text" placeholder="Price" name="post[price]"></div>
  <div><textarea name="post[description]" rows="10" cols="30" placeholder="Description"></textarea></div>
  <div><input type="text" placeholder="Location" name="post[location]"></div>
  <div><input type="submit"></div>
</form>
